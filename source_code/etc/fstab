# UNCONFIGURED FSTAB FOR BASE SYSTEM
/dev/root  /  ext4  noatime,errors=remount-ro,rw  0  1
mx-overlay   /var            fuse    nofail,defaults 0 0
mx-overlay   /etc            fuse    nofail,defaults 0 0
mx-overlay   /home            fuse    nofail,defaults 0 0
mx-overlay   /root            fuse    nofail,defaults 0 0
mx-overlay   /sbin            fuse    nofail,defaults 0 0
mx-overlay   /bin            fuse    nofail,defaults 0 0
mx-overlay   /usr            fuse    nofail,defaults 0 0
mx-overlay   /lib            fuse    nofail,defaults 0 0
